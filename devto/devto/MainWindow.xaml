<Window x:Name="Duck_Duck_go" x:Class="devto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:devto"
        mc:Ignorable="d"
        Title="DuckDuckGo" Height="450" Width="800" SizeToContent="WidthAndHeight">
    <Grid ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True">
        <WebBrowser x:Name="browserWindow" HorizontalAlignment="Left" Margin="0,6,-1,-1" VerticalAlignment="Top" Source="https://duckduckgo.com" ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True"/>
        <Label x:Name="goto_google" HorizontalAlignment="Left" Margin="381,13,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
