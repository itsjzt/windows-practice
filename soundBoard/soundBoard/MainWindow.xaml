<Window x:Class="soundBoard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:soundBoard"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button4" Content="Play Beep" HorizontalAlignment="Left" Margin="319,207,0,0" VerticalAlignment="Top" Width="75" Click="play_beep"/>
        <Button x:Name="button5" Content="Button" HorizontalAlignment="Left" Margin="477,208,0,0" VerticalAlignment="Top" Width="75" Click="play_sound5"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="649,201,0,0" VerticalAlignment="Top" Width="75" Click="play_sound"/>
        <Button x:Name="button2" Content="Button" HorizontalAlignment="Left" Margin="46,199,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.944,0.018" Click="play_sound2"/>
        <Button x:Name="button3" Content="Button" HorizontalAlignment="Left" Margin="180,201,0,0" VerticalAlignment="Top" Width="75" Click="play_sound3"/>

    </Grid>
</Window>
